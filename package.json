{
  "name": "@edraj/sveltestrap",
  "version": "6.0.1",
  "type": "module",
  "main": "dist/sveltestrap.js",
  "module": "dist/sveltestrap.es.js",
  "jsnext:main": "dist/sveltestrap.es.js",
  "svelte": "src/index.ts",
  "types": "src/index.d.ts",
  "keywords": [
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:edraj/sveltestrap.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "storybook dev -p 6006",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dist": "rollup -c --configPlugin @rollup/plugin-typescript",
    "docs": "storybook build -o docs && cp CNAME ./docs && cp -r ./v4 ./docs",
    "format": "prettier --write --plugin-search-dir=. ./src ./stories ./.storybook",
    "lint": "tsc --noEmit && eslint ./src --ext .ts --ext .svelte",
    "prepublish": "npm run dist && git add -A dist",
    "postpublish": "git push && git push --tags",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --maxWorkers=2 --verbose --coverage",
    "test:watch": "jest --maxWorkers=2 --verbose --watchAll",
    "version": "npm run dist && npm run docs && git add -A"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-actions": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addon-storysource": "^8.2.9",
    "@storybook/addons": "^7.6.17",
    "@storybook/blocks": "^8.2.9",
    "@storybook/svelte": "^8.2.9",
    "@storybook/svelte-webpack5": "^8.2.9",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^8.2.9",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/svelte": "^5.2.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-svelte": "^2.43.0",
    "get-port-cli": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.9.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prismjs": "^1.29.0",
    "raw-loader": "^4.0.2",
    "rollup": "^4.21.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-svelte": "^7.2.2",
    "sirv-cli": "^2.0.2",
    "storybook": "^8.2.9",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-eslint-parser": "^0.41.0",
    "svelte-jester": "^5.0.0",
    "svelte-loader": "^3.2.3",
    "svelte-preprocess": "^6.0.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4",
    "wait-for-localhost-cli": "^3.2.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8"
  },
  "browserslist": "last 2 versions"
}
